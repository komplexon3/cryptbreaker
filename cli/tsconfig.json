{
  "compilerOptions": {
    "target": "es5",
    "lib": ["esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "./build",
    "importHelpers": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [".", "../src/data", "../src/features/ciphers/*/utils", "../src/utils"],
  "extends": "./tsconfig.paths.json"
}
